    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.3",
    "nprogress": "^0.2.0",
    "react-hook-form": "^7.66.0",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "zod": "^4.1.12"
  }
